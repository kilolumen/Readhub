source 'https://github.com/CocoaPods/Specs.git'

platform:'ios','9.0'

inhibit_all_warnings!
use_frameworks!

def pods_for_targets

  pod 'Alamofire', '~> 4.5'
  pod 'RxSwift', '~> 4.0'
  pod 'RxCocoa', '~> 4.0'
  pod 'Moya'
  pod 'SnapKit', '~> 4.0.0'
  pod 'Alamofire-SwiftyJSON'
  pod 'SwiftyJSON'
  pod 'ObjectMapper', '~> 3.1'

end

target 'Readhub' do
  pods_for_targets
end

